{
  "name": "fullstack-typescript",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.16.1"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "lint-fix": "eslint 'src/**/*.{ts,tsx}' --fix --max-warnings=0",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "pretty": "prettier --write \"src/**/*.{ts,tsx}\"",
    "start:dev": "node index.js",
    "start": "cross-env NODE_ENV=production node index.js",
    "type-check-client": "tsc -p ./src/client/tsconfig.json",
    "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --mode production",
    "build-server": "tsc -b src/server",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve",
    "dev-server": "mkdir -p dist/server & ts-purify -s src/server -d dist/server -w & tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"node --enable-source-maps index.js\"",
    "dev-server-old-no-purify": "tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"node --enable-source-maps index.js\"",
    "dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "rm -rf dist/* && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\"",
    "nuke-dependencies": "rm -rf node_modules rm -f package-lock.json",
    "delete-dependencies": "rm -rf node_modules",
    "dangerously-rebuild": "npm run nuke-dependencies && npm install && npm run build",
    "migration:up": "npx mikro-orm migration:up",
    "migration:create": "npx mikro-orm migration:create",
    "db:populate": "node dist/server/setup/database-population/index.js",
    "gen": "graphql-codegen --config codegen.yml",
    "initdb": "babel-node setup/initdb.js",
    "dumpdb": "babel-node setup/dumpdb.js",
    "force-publish": "heroku pg:reset && heroku pg:push fullstack-typescript DATABASE_URL --app fullstack-typescript-web; git push heroku master",
    "prepare": "husky install",
    "coverage": "npm test -- --coverage --watchAll=false"
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jbccollins/fullstack-typescript.git"
  },
  "license": "MIT",
  "author": "James Collins",
  "jest": {
    "coverageReporters": [
      "json",
      "text",
      "html",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/server/**/*/*.ts",
      "src/shared/**/*/*.ts",
      "src/client/**/*/*.{ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "@shared/(.*)": "<rootDir>/src/shared/$1",
      "@client/(.*)": "<rootDir>/src/client/$1",
      "@server/(.*)": "<rootDir>/src/server/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/client/setupEnzyme.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@server": "dist/server",
    "@client": "dist/client"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/server/database/orm/mikro-orm.config.ts",
      "./dist/server/database/orm//mikro-orm.config.js"
    ]
  },
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@mikro-orm/cli": "^4.5.4",
    "@mikro-orm/core": "^4.5.4",
    "@mikro-orm/migrations": "^4.5.4",
    "@mikro-orm/postgresql": "^4.5.4",
    "@reduxjs/toolkit": "^1.5.1",
    "@types/ejs": "^3.0.6",
    "@urql/exchange-graphcache": "^4.1.3",
    "@webpack-cli/serve": "^1.2.2",
    "apollo-server-express": "^2.24.0",
    "argon2": "^0.27.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "find-up": "^5.0.0",
    "formik": "^2.2.9",
    "graphql": "^15.5.0",
    "ioredis": "^4.27.6",
    "nodemailer": "^6.6.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "2.1.0",
    "styled-components": "^5.2.1",
    "tslib": "^2.1.0",
    "type-graphql": "^1.1.1",
    "urql": "^2.0.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-numeric-separator": "^7.12.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@graphql-codegen/typescript-urql": "^2.0.7",
    "@material-ui/core": "^4.11.3",
    "@types/connect-redis": "0.0.16",
    "@types/cors": "^2.8.10",
    "@types/cssnano": "^4.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.2",
    "@types/pg": "^7.14.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redis": "^2.8.28",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@types/webpack": "^4.41.26",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@types/webpack-dev-server": "^3.11.4",
    "@types/webpack-env": "^1.16.0",
    "@types/webpack-manifest-plugin": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@urql/devtools": "^2.0.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "class-validator": "^0.13.1",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.3",
    "cosmiconfig": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^5.0.6",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "file-loader": "^6.2.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "module-alias": "^2.2.2",
    "postcss": "^8.2.14",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-devtools": "^3.7.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-purify": "^3.0.1",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.22.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.0.0"
  }
}
